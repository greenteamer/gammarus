{
  "name": "mobx-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 compilation and hot code reloading",
  "scripts": {
    "lint": "node_modules/.bin/eslint src/** --ext .js,.jsx",
    "build": "node_modules/.bin/webpack --progress -d --watch",
    "dev": "webpack-dev-server --open --inline --hot --env dev",
    "start": "npm run build",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Michel Weststrate <mweststrate@gmail.com> (http://github.com/mweststrate)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^2.13.1",
    "eslint-plugin-flowtype": "^2.25.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.34.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bootstrap": "^4.0.0-alpha.5",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "mobx": "^2.2.2",
    "singleton": "^1.0.0",
    "tether": "^1.3.7",
    "underscore": "^1.8.3"
  }
}
